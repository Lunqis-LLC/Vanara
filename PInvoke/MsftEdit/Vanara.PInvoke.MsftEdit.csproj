<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <ProjectExtensions>
    <SupportedDlls>msftedit.dll,riched20.dll</SupportedDlls>
  </ProjectExtensions>
  <PropertyGroup>
    <Description>PInvoke API (methods, structures and constants) imported from Windows MsftEdit.dll for the Rich Edit control.</Description>
    <AssemblyName>Vanara.PInvoke.MsftEdit</AssemblyName>
    <AssemblyTitle>$(AssemblyName)</AssemblyTitle>
    <PackageId>$(AssemblyName)</PackageId>
    <PackageTags>pinvoke;vanara;net-extensions;interop;MsftEdit;rich edit</PackageTags>
    <PackageReleaseNotes />
    <PackageReadmeFile>pkgreadme.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Core\Vanara.Core.csproj" />
    <ProjectReference Include="..\Imm32\Vanara.PInvoke.Imm32.csproj" />
    <ProjectReference Include="..\Shared\Vanara.PInvoke.Shared.csproj" />
    <ProjectReference Include="..\Ole\Vanara.PInvoke.Ole.csproj" />
    <ProjectReference Include="..\User32\Vanara.PInvoke.User32.csproj" />
  </ItemGroup>
</Project>