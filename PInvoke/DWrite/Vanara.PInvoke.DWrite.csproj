  <ProjectExtensions>
    <SupportedDlls>DWrite.dll</SupportedDlls>
  </ProjectExtensions>
  <PropertyGroup>
    <Description>PInvoke API (methods, structures and constants) imported from Windows DWrite.dll.</Description>
    <AssemblyName>Vanara.PInvoke.DWrite</AssemblyName>
