<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <ProjectExtensions>
    <SupportedDlls>d3d12.dll</SupportedDlls>
  </ProjectExtensions>
  <PropertyGroup>
    <Description>PInvoke API (methods, structures and constants) imported from Windows Direct3D 12.</Description>
    <AssemblyName>Vanara.PInvoke.Direct3D12</AssemblyName>
    <AssemblyTitle>$(AssemblyName)</AssemblyTitle>
    <PackageId>$(AssemblyName)</PackageId>
    <PackageTags>pinvoke;vanara;net-extensions;interop;Direct3D12</PackageTags>
    <PackageReleaseNotes />
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Core\Vanara.Core.csproj" />
    <ProjectReference Include="..\D3DCompiler\Vanara.PInvoke.D3DCompiler.csproj" />
    <ProjectReference Include="..\Direct3D11\Vanara.PInvoke.Direct3D11.csproj" />
    <ProjectReference Include="..\Shared\Vanara.PInvoke.Shared.csproj" />
  </ItemGroup>
</Project>