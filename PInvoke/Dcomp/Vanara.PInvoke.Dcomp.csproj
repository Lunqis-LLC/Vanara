<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <ProjectExtensions>
    <SupportedDlls>Dcomp.dll</SupportedDlls>
  </ProjectExtensions>
  <PropertyGroup>
    <Description>PInvoke API (methods, structures and constants) imported from Windows Dcomp.dll (DirectComposition).</Description>
    <AssemblyName>Vanara.PInvoke.Dcomp</AssemblyName>
    <AssemblyTitle>$(AssemblyName)</AssemblyTitle>
    <PackageId>$(AssemblyName)</PackageId>
    <PackageTags>pinvoke;vanara;net-extensions;interop;dcomp;directcomposition</PackageTags>
    <PackageReleaseNotes />
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Direct2D\Vanara.PInvoke.Direct2D.csproj" />
  </ItemGroup>
</Project>